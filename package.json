{
  "name": "react-i18n-audit",
  "version": "0.0.1",
  "description": "Audit i18n keys for React/Next: scan, report, prune",
  "type": "module",
  "private": false,
  "bin": {
    "react-i18n-audit": "dist/cli.cjs"
  },
  "scripts": {
    "build": "tsup src/cli.ts --format esm,cjs --dts --splitting --clean",
    "dev": "tsx src/cli.ts",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "vitest",
    "prepublishOnly": "npm run build"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "i18n",
    "react",
    "nextjs",
    "audit",
    "translations",
    "cli"
  ],
  "author": "Knexy04",
  "license": "MIT",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "restricted"
  },
  "devDependencies": {
    "@types/babel__traverse": "^7.28.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.7.4",
    "eslint": "^9.12.0",
    "tsup": "^8.3.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3",
    "vitest": "^2.1.3"
  },
  "dependencies": {
    "@babel/parser": "^7.25.8",
    "@babel/traverse": "^7.25.7",
    "commander": "^12.1.0",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "zod": "^3.23.8"
  }
}
